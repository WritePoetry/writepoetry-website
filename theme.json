		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--source-code-pro)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "50px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--black)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--base)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--base)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--base)"
					}
				},
				"typography": {
					"fontWeight": "700"
				}
			}
		}
	}
}