{
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#eeeeee",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#361C1C",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#FA6900",
					"name": "Primary",
					"slug": "primary"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Source Code Pro",
					"slug": "source-code-pro",
					"fontFace": [
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/source-code-pro_normal_200.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/source-code-pro_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
